package com.bajaj.day2;

class A
{
	int a=10;
	A(int a)
	{
		this.a=a;
	}
	public void printA()
	{
		System.out.println("A class");
	}
	
}

class B extends A
{
	public void printB()
	{
		System.out.println("B class " +a);
	}
	B(int a)
	{
		super(a);
	}
}

public class MyInheritance {
	public static void main(String args[])
	{
		B obj=new B();
		obj.printA();
		obj.printB();
	}

}