package com.bajaj.day2;



class A
{
	public void printA()
	{
		System.out.println("A class");
	}
	
}

class B extends A
{
	public void printB()
	{
		System.out.println("B class");
	}
}

public class MyInheritance {
	public static void main(String args[])
	{
		B obj=new B();
		obj.printA();
		obj.printB();
	}

}