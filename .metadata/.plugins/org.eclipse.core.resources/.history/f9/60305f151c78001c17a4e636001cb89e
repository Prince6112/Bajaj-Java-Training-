package com.bajaj.day2;

class Person
{
	public String name;
	private int age;
	private int salary;
	
	Person (String name,int age,int salary)
	{
		this.name=name;
		this.age=age;
		this.salary=salary;
	}
}


public class MyMain
{
	public static void main(String args[])
	{
		Person ABC=new Person("Prince",21,10000);
		
		
	}
}